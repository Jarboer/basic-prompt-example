{
  "name": "basic-prompt-example",
  "productName": "Basic Prompt Example",
  "version": "0.0.1",
  "description": "This project is a basic program to test my prompt library (fork)",
  "author": "Jarboer",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jarboer/basic-prompt-example.git"
  },
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "license": "ISC",
  "main": "dist/main/main.bundle.js",
  "private": true,
  "scripts": {
    "start": "electron-forge start",
    "build": "pnpm webpack",
    "webpack": "pnpx webpack-cli --config webpack.config.js"
  },
  "keywords": [
    "Prompt",
    "Example",
    "Test"
  ],
  "build": {
    "appId": "com.jarboer.basic-prompt-example",
    "directories": {
      "output": "out"
    },
    "files": [
      "dist/**",
      "package.json"
    ],
    "win": {
      "target": "NSIS",
      "icon": "resources/icons/appIcon.ico",
      "publish": "github"
    }
  },
  "dependencies": {
    "@jarboer/electron-prompt": "^1.8.0"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^12.0.2",
    "electron": "^25.6.0",
    "html-webpack-plugin": "^5.6.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
